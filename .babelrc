{
  "presets": [
    [
      "next/babel",
      {
        "styled-jsx": {
          "plugins": ["@styled-jsx/plugin-sass"]
        }
      }
    ]
  ],
  "plugins": [
    ["module-resolver", {
      "root": ["."],
      "alias": {
        "@md": "./md",
        "@components": "./components",
        "@styles": "./styles",
        "@utils": "./utils",
        "@workers": "./workers",
        "@typings": "./typings",
        "@assets":  "./assets",
        "@constants": "./constants",
        "@hooks": "./hooks"
      }
    }]
  ]
}
